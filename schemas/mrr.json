{
"$schema": "http://json-schema.org/draft-04/schema#",
"title": "mrr",
"description": "Schema for Materials Resource Registry integration.",
"type": "object",
"properties": {
    "mrr": {
        "type": "object",
        "description": "NIST Materials Resource Registry metadata.",
        "properties": {
            "dataOrigin":{
                "type": "array",
                "items": {
                    "type": "string",
                    "description": "A category reflecting broadly how the data was produced."
                }
            },
            "materialType": {
                "type": "array",
                "items": {
                    "type": "string",
                    "description": "The category of material studied in the data contained in this resource."
                }
            },
            "structuralFeature": {
                "type": "array",
                "items": {
                    "type": "string",
                    "description": "The primary or prevalent characteristic of the structure of the material of interest to this resource."
                }
            },
            "propertiesAddressed": {
                "type": "array",
                "items": {
                    "type": "string",
                    "description": "A category of property that is sampled by the contained data."
                }
            },
            "characterizationMethod": {
                "type": "array",
                "items": {
                    "type": "string",
                    "description": "The experimental technique used to acquire the data within this resource."
                }
            },
            "computationalMethod": {
                "type": "array",
                "items": {
                    "type": "string",
                    "description": "The computational technique used to acquire the data within this resource."
                }
            },
            "computeScale": {
                "type": "array",
                "items": {
                    "type": "string",
                    "description": "The physical scale that is represented by a computation."
                }
            },
            "synthesisProcessing": {
                "type": "array",
                "items": {
                    "type": "string",
                    "description": "The physical processing or preparation technique applied to the material being studied."
                }
            }
        },
        "additionalProperties": true
    }
}
}
